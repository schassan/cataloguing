            <attribute xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                       name="status"
                       a:defaultValue="unknown">
               <a:documentation>(status) supplies a code identifying the current availability of the text.</a:documentation>
               <choice>
                  <value>free</value>
                  <a:documentation/>
                  <value>unknown</value>
                  <a:documentation/>
                  <value>restricted</value>
                  <a:documentation/>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="TEI_licence">
