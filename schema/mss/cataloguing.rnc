  }
TEI_physDesc =
  
  ## (physical description) contains a full physical description of a manuscript or manuscript part, optionally subdivided using more specialized elements from the model.physDescPart class. [10.7. Physical Description]
  element physDesc {
    (TEI_model.pLike*,
     TEI_objectDesc?,
     TEI_handDesc?,
     TEI_typeDesc?,
     TEI_scriptDesc?,
     TEI_musicNotation?,
     TEI_decoDesc?,
     TEI_additions?,
     TEI_bindingDesc?,
     TEI_sealDesc?,
     TEI_accMat?),
    TEI_att.global.attributes,
    empty
  }
TEI_objectDesc =
  
  ## contains a description of the physical components making up the object which is being described. [10.7.1. Object Description]
  element objectDesc {
    (TEI_model.pLike+ | (TEI_supportDesc?, TEI_layoutDesc?)),
    TEI_att.global.attributes,
    
    ## a short project-specific name identifying the physical form of the carrier, for example as a codex, roll, fragment, partial leaf, cutting etc.
    [ a:defaultValue = "codex" ]
    attribute form {
      
      ## a bound codex
      "codex"
      | 
        ## a charter
        "charter"
      | 
        ## part of a bound codex with its own history
        "fascicle"
      | 
        ## a fragment of a codex
        "fragment"
      | 
        ## a loose leaf
        "leaf"
      | 
        ## a letter, possibly part of a bound codex now, i.e. a composite manuscript
        "letter"
      | 
        ## a scroll
        "scroll"
      | 
        ## any other format
        "other"
    },
    empty
  }
TEI_supportDesc =
  
  ## (support description) groups elements describing the physical support for the written part of a manuscript. [10.7.1. Object Description]
  element supportDesc {
    (TEI_model.pLike+
     | (TEI_support?,
        TEI_extent?,
        TEI_foliation*,
        TEI_collation?,
        TEI_condition?)),
    TEI_att.global.attributes,
    
    ## a short project-defined name for the material composing the majority of the support
    ## Suggested values include: 1] perg; 2] chart; 3] perg_chart; 4] linen; 5] mixed; 6] palm; 7] papyrus; 8] papyrus_perg; 9] papyrus_chart; 10] unknown
    [ a:defaultValue = "unknown" ]
    attribute material {
      
      ## parchment
      "perg"
      | 
        ## paper
        "chart"
      | 
        ## mixture of paper and parchment
        "perg_chart"
      | 
        ## linen
        "linen"
      | 
        ## mixture of any other materials than paper, papyrus, and parchment
        "mixed"
      | 
        ## palm
        "palm"
      | 
        ## papyrus
        "papyrus"
      | 
        ## mixture of papyrus and parchment
        "papyrus_perg"
      | 
        ## mixture of papyrus and paper
        "papyrus_chart"
      | 
        ## unknown
        "unknown"
      | xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }
    },
    empty
  }
TEI_support =
  
  ## contains a description of the materials etc. which make up the physical support for the written part of a manuscript. [10.7.1. Object Description]
  element support {
