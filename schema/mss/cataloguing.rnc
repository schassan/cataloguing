    [ a:defaultValue = "other" ]
    attribute type {
      
      ##
      "border"
      | 
        ##
        "diagram"
      | 
        ##
        "initial"
      | 
        ##
        "marginal"
      | 
        ##
        "miniature"
      | 
        ##
        "mixed"
      | 
        ##
        "paratext"
      | 
        ##
        "secondary"
      | 
        ##
        "other"
      | 
        ##
        "illustration"
      | 
        ##
        "printmark"
      | 
        ##
        "publishmark"
      | 
        ##
        "vignette"
      | 
        ##
        "frieze"
      | 
        ##
        "map"
      | 
        ##
        "unspecified"
