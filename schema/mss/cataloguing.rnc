    TEI_att.global.change.attribute.change,
    TEI_att.global.responsibility.attribute.cert,
    TEI_att.global.responsibility.attribute.resp,
    TEI_att.global.source.attribute.source,
    TEI_att.written.attributes,
    
    ## (rendition) indicates how the element in question was rendered or presented in the source text.
    ## Suggested values include: 1] font-stretch:expanded; 2] font-style:italic; 3] font-style:normal; 4] font-variant:small-caps; 5] font-weight:bold; 6] text-decoration:underline; 7] text-decoration:double-underline; 8] hyphenated; 9] rubric; 10] stacked; 11] sup
    attribute rend {
      list {
        (
         ##
         "font-stretch:expanded"
         | 
           ##
           "font-style:italic"
         | 
           ##
           "font-style:normal"
         | 
           ##
           "font-variant:small-caps"
         | 
           ##
           "font-weight:bold"
         | 
           ##
           "text-decoration:underline"
         | 
           ##
           "text-decoration:double-underline"
         | 
           ##
           "hyphenated"
         | 
           ##
           "rubric"
         | 
           ##
           "stacked"
         | 
           ##
           "sup"
